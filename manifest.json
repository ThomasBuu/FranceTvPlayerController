{
    "manifest_version": 3,
    "name": "Video Player Controls Manager",
    "version": "1.0",
    "description": "Gérez l'affichage des contrôles du lecteur vidéo",
    "permissions": ["activeTab", "storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
        "16": "etoiles.png",
        "32": "etoiles.png",
        "48": "etoiles.png",
        "128": "etoiles.png"
      },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  